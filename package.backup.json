{
  "name": "zyromarketplace",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "preview": "node start-preview.js",
    "chrome": "node start-chrome-preview.js",
    "mobile-preview": "node launch-mobile-preview.js",
    "web": "expo start --web",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "build:production": "expo build --release-channel production",
    "build:production:windows": "powershell -ExecutionPolicy Bypass -File scripts/build-production.ps1",
    "submit:stores": "chmod +x scripts/submit-stores.sh && ./scripts/submit-stores.sh",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "expo:deploy": "chmod +x scripts/expo-deploy.sh && ./scripts/expo-deploy.sh",
    "expo:deploy:windows": "powershell -ExecutionPolicy Bypass -File scripts/expo-deploy.ps1",
    "expo:check": "chmod +x scripts/pre-deployment-check.sh && ./scripts/pre-deployment-check.sh",
    "expo:login": "eas login",
    "expo:whoami": "eas whoami",
    "expo:builds": "eas build:list",
    "expo:submissions": "eas submit:list",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:staging": "expo build --release-channel staging",
    "build:android:staging": "expo build:android --release-channel staging",
    "build:android:production": "expo build:android --release-channel production",
    "build:ios:staging": "expo build:ios --release-channel staging",
    "build:ios:production": "expo build:ios --release-channel production",
    "publish:staging": "expo publish --release-channel staging",
    "publish:production": "expo publish --release-channel production",
    "deploy:staging": "npm run test && npm run build:staging && npm run publish:staging",
    "deploy:production": "npm run test && npm run build:production && npm run publish:production",
    "prebuild": "npm run test:coverage",
    "postbuild": "npm run analyze-bundle",
    "analyze-bundle": "echo 'Bundle analysis skipped'",
    "clean": "expo r -c",
    "clean:cache": "expo r -c && npm start -- --reset-cache",
    "doctor": "expo doctor",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-firebase/app": "^23.1.2",
    "@react-native-firebase/messaging": "^23.1.2",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-query": "^5.85.5",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "expo": "~53.0.22",
    "expo-build-properties": "^0.14.8",
    "expo-camera": "^16.1.11",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "^5.2.4",
    "expo-location": "^18.1.6",
    "expo-media-library": "^17.1.7",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.5",
    "expo-sqlite": "~15.2.14",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-map-clustering": "^4.0.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "react-redux": "^9.2.0",
    "styled-components": "^6.1.19",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/react-native-maps": "^0.24.1",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/styled-components-react-native": "^5.2.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
