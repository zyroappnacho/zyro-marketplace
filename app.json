{
  "expo": {
    "name": "ZYRO Marketplace",
    "slug": "zyro-marketplace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logozyrotransparente.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logozyrotransparente.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zyro.marketplace",
      "buildNumber": "2",
      "deploymentTarget": "13.0",
      "requireFullScreen": false,
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch",
          "remote-notification"
        ],
        "NSCameraUsageDescription": "Esta app necesita acceso a la cámara para subir fotos de contenido de colaboraciones.",
        "NSPhotoLibraryUsageDescription": "Esta app necesita acceso a la galería para seleccionar fotos de contenido de colaboraciones.",
        "NSLocationWhenInUseUsageDescription": "Esta app necesita acceso a la ubicación para mostrar colaboraciones cercanas en el mapa.",
        "NSUserNotificationUsageDescription": "Esta app envía notificaciones sobre nuevas colaboraciones y actualizaciones importantes.",
        "UILaunchStoryboardName": "SplashScreen",
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "UIViewControllerBasedStatusBarAppearance": false,
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "simulator": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logozyrotransparente.png",
        "backgroundColor": "#000000"
      },
      "package": "com.zyro.marketplace",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/logozyrotransparente.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-notifications",
      "expo-location",
      [
        "expo-image-picker",
        {
          "photosPermission": "La app accede a tus fotos para permitir subir contenido de colaboraciones.",
          "cameraPermission": "La app accede a tu cámara para permitir tomar fotos de contenido de colaboraciones."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Regular.ttf",
            "./assets/fonts/Inter-Bold.ttf"
          ]
        }
      ]
    ],
    "notification": {
      "icon": "./assets/logozyrotransparente.png",
      "color": "#C9A961",
      "androidMode": "default",
      "androidCollapsedTitle": "ZYRO Marketplace"
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id-here"
      }
    },
    "owner": "your-expo-username",
    "privacy": "unlisted",
    "scheme": "zyro-marketplace"
  }
}