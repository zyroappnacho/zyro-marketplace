# Staging Environment Variables
NODE_ENV=staging

# API Configuration
API_BASE_URL=https://staging-api.zyromarketplace.com
API_TIMEOUT=30000

# Database Configuration
DATABASE_URL=staging_database_url_here
DATABASE_POOL_SIZE=10
DATABASE_TIMEOUT=30000

# Firebase Configuration
FIREBASE_API_KEY=staging_firebase_api_key
FIREBASE_AUTH_DOMAIN=zyromarketplace-staging.firebaseapp.com
FIREBASE_PROJECT_ID=zyromarketplace-staging
FIREBASE_STORAGE_BUCKET=zyromarketplace-staging.appspot.com
FIREBASE_MESSAGING_SENDER_ID=staging_sender_id
FIREBASE_APP_ID=staging_app_id

# Payment Configuration (Test Mode)
STRIPE_PUBLISHABLE_KEY=pk_test_staging_key
PAYMENT_WEBHOOK_SECRET=whsec_staging_webhook_secret

# Security
JWT_SECRET=staging_jwt_secret_key_here
ENCRYPTION_KEY=staging_encryption_key_here

# Logging
LOG_LEVEL=warn
SENTRY_DSN=https://staging_sentry_dsn_here

# Performance
ENABLE_PERFORMANCE_MONITORING=true
CACHE_TTL=180000

# Admin Credentials
ADMIN_USERNAME_ENCRYPTED=encrypted_staging_admin_username
ADMIN_PASSWORD_ENCRYPTED=encrypted_staging_admin_password

# App Configuration
APP_VERSION=1.0.0-staging
BUILD_NUMBER=1