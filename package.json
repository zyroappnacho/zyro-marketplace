{
  "name": "zyromarketplace",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node start-render.js",
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "ios:enhanced": "./start-ios-enhanced.sh",
    "ios:clean": "./start-ios-enhanced.sh --clean",
    "ios:fix": "./fix-ios-simulator.sh",
    "ios:recreate": "./fix-ios-simulator.sh",
    "web": "expo start --web",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "preview:ios": "eas build --profile preview --platform ios",
    "preview:android": "eas build --profile preview --platform android",
    "update": "eas update",
    "test": "jest",
    "test:ios": "./test-ios-complete.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "simulator:list": "xcrun simctl list devices",
    "simulator:reset": "xcrun simctl erase all",
    "cache:clear": "rm -rf .expo .metro-cache node_modules/.cache && npm cache clean --force",
    "zyro:start": "./start-ios-enhanced.sh",
    "zyro:clean": "./start-ios-enhanced.sh --clean",
    "zyro:recreate": "./fix-ios-simulator.sh",
    "zyro:test": "./test-ios-complete.sh",
    "start:premium": "expo start --clear",
    "ios:premium": "expo start --ios --clear",
    "android:premium": "expo start --android --clear",
    "web:premium": "expo start --web --clear"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@reduxjs/toolkit": "^2.0.1",
    "@stripe/stripe-js": "^2.4.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "expo": "~53.0.22",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "^6.0.13",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.1.1",
    "express": "^5.1.0",
    "fs-extra": "^11.3.1",
    "node-fetch": "^3.3.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.13.0",
    "react-native-web": "^0.20.0",
    "react-native-worklets": "^0.5.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "stripe": "^14.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@types/react": "~19.0.10",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
